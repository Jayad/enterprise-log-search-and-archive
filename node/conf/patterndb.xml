	<ruleset name="PIX" id='2_PIX'>
		<pattern>%PIX</pattern>
		<rules>
			<rule provider="DET" class='2' id='2'>
				<patterns>
					<pattern>Deny@QSTRING:i0: @src@QSTRING:s0: :@@IPv4:i1:@/@NUMBER:i2:@ dst@QSTRING:s1: :@@IPv4:i3:@/@NUMBER:i4:@ by access-group @QSTRING:s2:"@</pattern>
				</patterns>
